<!-- 图片轮播-->
<view class="slider">
    <swiper autoplay interval="3500" indicator-dots="false">
        <swiper-item wx:for="{{imageslist}}" wx:key="id">
        <image src="{{item.img_url}}" mode='aspectFill' lazy-load width="375" height="150" />
        </swiper-item>
    </swiper>
</view>
